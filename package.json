  "scripts": {
    "heroku-postbuild": "npm run build",
    "build-scss": "node-sass-chokidar ./src -o ./src",
    "watch-css": "npm run build-scss && node-sass-chokidar ./src -o ./src --watch --recursive",
    "start-js": "node ./node_modules/react-scripts/bin/react-scripts.js start",
    "start": "npm start --prefix server",
    "start-dev": "npm-run-all -p watch-css start-js",
    "build-js": "CI=false && react-scripts build",
    "build": "CI=false && npm rebuild node-sass npm-run-all build-scss build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
